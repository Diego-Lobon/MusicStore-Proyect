end$$

CREATE DEFINER=`` PROCEDURE `Actualizar_Usuario_Metodo` (IN `metodoN` VARCHAR(150), IN `ipU` VARCHAR(150))  begin
	UPDATE informacionPago SET metodo = metodoN WHERE ip = ipU;
end$$

CREATE DEFINER=`` PROCEDURE `Buscador_Producto` (`producto` VARCHAR(1000))  begin
	SELECT * FROM productos WHERE nombre LIKE CONCAT('%',producto,'%');
end$$

CREATE DEFINER=`` PROCEDURE `Buscar_Carrito` (IN `nombreProducto` VARCHAR(150), IN `ipUsuario` VARCHAR(150))  begin
	DECLARE idUsuarioN, idProductoN, cant INT;
	SET idUsuarioN = (SELECT idUsuario FROM usuarios WHERE ip = ipUsuario);
    SET idProductoN = (SELECT idProducto FROM productos WHERE nombre = nombreProducto);
	SELECT * from carrito_de_compras WHERE idUsuario = idUsuarioN AND idProducto = idProductoN;
end$$

CREATE DEFINER=`` PROCEDURE `Buscar_Producto` (`nombreProducto` VARCHAR(100))  begin
	SELECT * FROM productos WHERE nombreProducto = nombre;
end$$

CREATE DEFINER=`` PROCEDURE `Buscar_Producto_ID` (`id` VARCHAR(100))  begin
	SELECT * FROM productos WHERE idProducto = id;
end$$

CREATE DEFINER=`` PROCEDURE `Buscar_Usuario` (IN `ipUsuario` VARCHAR(150))  begin
	SELECT * FROM usuarios WHERE ipUsuario = ip;
end$$

CREATE DEFINER=`` PROCEDURE `Buscar_Usuario_Informacion` (`ipUsuario` VARCHAR(150))  begin
	SELECT * FROM informacionPago WHERE ip = ipUsuario;
end$$

CREATE DEFINER=`` PROCEDURE `Cantidad_Registros_Carrito` ()  begin
	SELECT count(*) as cantidad FROM ventas;
end$$

CREATE DEFINER=`` PROCEDURE `Crear_Usuario` (IN `nombre` VARCHAR(45), IN `apellido` VARCHAR(45), IN `correo` VARCHAR(45), IN `contraseña` VARCHAR(45), IN `tipo` VARCHAR(45), IN `activo` VARCHAR(60), IN `ip` VARCHAR(150))  begin
	INSERT INTO usuarios(nombre, apellido, correo, contraseña, tipo, activo, ip)
    VALUES(nombre, apellido, correo, contraseña, tipo, activo, ip);
end$$

CREATE DEFINER=`` PROCEDURE `Crear_Usuario_Informacion` (IN `nombre` VARCHAR(150), IN `apellido` VARCHAR(150), IN `direccion` VARCHAR(150), IN `apartamento` VARCHAR(150), IN `distrito` VARCHAR(150), IN `region` VARCHAR(150), IN `celular` VARCHAR(150), IN `correo` VARCHAR(150), IN `metodo` VARCHAR(150), IN `dni` VARCHAR(150), IN `ip` VARCHAR(150))  begin
	INSERT INTO informacionPago(nombre, apellido, direccion, apartamento, distrito, region, celular, correo, ip, metodo, dni)
    VALUES(nombre, apellido, direccion, apartamento, distrito, region, celular, correo, ip, metodo, dni);
end$$

CREATE DEFINER=`` PROCEDURE `Eliminar_Producto` (`idCarritoE` INT)  begin
	DELETE FROM carrito_de_compras WHERE idCarrito = idCarritoE;
end$$

CREATE DEFINER=`` PROCEDURE `Historial_Pedido_Usuario` (`ipUsuario` VARCHAR(150))  begin
	DECLARE idUsuarioN INT;
	SET idUsuarioN = (SELECT idUsuario FROM usuarios WHERE ip = ipUsuario);
	SELECT P.nombre, V.CantidadProducto, V.total, V.estadoEntrega, V.fecha FROM ventas V
    inner join productos P on P.idProducto = V.idProducto WHERE V.idUsuario = idUsuarioN;
end$$

CREATE DEFINER=`` PROCEDURE `Insertar_Producto_Carrito` (IN `nombreProducto` VARCHAR(150), IN `ipUsuario` VARCHAR(150), IN `cantidad` INT, IN `totalPrecio` DOUBLE)  begin 
	DECLARE idUsuarioN, idProductoN, cant INT;
	SET idUsuarioN = (SELECT idUsuario FROM usuarios WHERE ip = ipUsuario);
    SET idProductoN = (SELECT idProducto FROM productos WHERE nombre = nombreProducto);
    SET cant = (SELECT count(*) from carrito_de_compras WHERE idUsuario = idUsuarioN AND idProducto = idProductoN);
    IF (cant = 0) THEN
		INSERT INTO carrito_de_compras(idUsuario, idProducto, cantidadProducto, total)
		VALUES(idUsuarioN, idProductoN, cantidad, totalPrecio);
	ELSE
		UPDATE carrito_de_compras SET cantidadProducto = cantidad WHERE idUsuario = idUsuarioN AND idProducto = idProductoN;
        UPDATE carrito_de_compras SET total = totalPrecio WHERE idUsuario = idUsuarioN AND idProducto = idProductoN;
	END IF;	
end$$

CREATE DEFINER=`` PROCEDURE `Limpiar_Carrito_Usuario` (IN `ipUsuario` VARCHAR(150))  begin
	DECLARE idUsuarioN INT;
	SET idUsuarioN = (SELECT idUsuario FROM usuarios WHERE ip = ipUsuario);
	delete from carrito_de_compras where idUsuario=idUsuarioN;
end$$

CREATE DEFINER=`` PROCEDURE `Limpiar_Informacion_Usuario` (IN `ipUsuario` VARCHAR(150))  begin
	delete from informacionPago where ip=ipUsuario;
end$$

CREATE DEFINER=`` PROCEDURE `Listar_Carrito` ()  begin
	SELECT * FROM carrito_de_compras;
end$$

CREATE DEFINER=`` PROCEDURE `Listar_Carrito_Productos` (IN `ipUsuario` VARCHAR(150))  begin
	DECLARE idUsuarioN INT;
	SET idUsuarioN = (SELECT idUsuario FROM usuarios WHERE ip = ipUsuario);
	SELECT  C.idCarrito , C.idUsuario ,C.idProducto, P.nombre, P.descripcion, P.precio, P.marca, P.modelo, P.categoria, P.tipo, P.cantidad, C.cantidadProducto, C.total FROM carrito_de_compras C
    inner join productos P on P.idProducto = C.idProducto WHERE idUsuario = idUsuarioN;
end$$

CREATE DEFINER=`` PROCEDURE `Listar_Marcas` (`categoriaN` VARCHAR(150))  begin
