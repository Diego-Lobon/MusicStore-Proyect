CREATE DEFINER=`` PROCEDURE `Registrar_Usuario` (IN `nombre` VARCHAR(150), IN `apellido` VARCHAR(150), IN `correo` VARCHAR(150), IN `contraseña` VARCHAR(150))  begin
	INSERT INTO usuarios (nombre, apellido, correo, contraseña, tipo, activo, ip)
    VALUES (nombre, apellido, correo, contraseña, "Registrado", "No Activo", nombre);
end$$

CREATE DEFINER=`` PROCEDURE `Registrar_Usuario_N` (IN `nombre` VARCHAR(150), IN `apellido` VARCHAR(150), IN `correo` VARCHAR(150), IN `contraseña` VARCHAR(150))  begin
	DECLARE idUsuarioN INT;
    SET idUsuarioN = (SELECT last_insert_id() FROM usuarios limit 1);
	INSERT INTO usuarios (nombre, apellido, correo, contraseña, tipo, activo, ip)
    VALUES (nombre, apellido, correo, contraseña, "Registrado", "No Activo", idUsuarioN+1);
end$$

CREATE DEFINER=`` PROCEDURE `Registrar_Venta` (`idVentaMP` INT, `idUsuario` INT, `estado` VARCHAR(100), `tipoPago` VARCHAR(100), `idOrdenComercial` INT, `idProducto` INT, `CantidadProducto` INT, `total` INT, `nombre` VARCHAR(200), `apellido` VARCHAR(200), `direccion` VARCHAR(200), `apartamento` VARCHAR(200), `distrito` VARCHAR(200), `region` VARCHAR(200), `celular` VARCHAR(200), `correo` VARCHAR(200), `ip` VARCHAR(200), `metodo` VARCHAR(200), `dni` VARCHAR(200))  begin
	INSERT INTO ventas (idVentaMP, idUsuario, estado, tipoPago, idOrdenComercial, idProducto, CantidadProducto, total, nombre, apellido, direccion, apartamento, distrito, region, celular, correo, ip, metodo, dni, estadoEntrega, fecha)
    VALUES (idVentaMP, idUsuario, estado, tipoPago, idOrdenComercial, idProducto, CantidadProducto, total, nombre, apellido, direccion, apartamento, distrito, region, celular, correo, ip, metodo, dni, 'En Proceso', current_timestamp());
end$$

CREATE DEFINER=`` PROCEDURE `Sub_Total_Carrito` (IN `ipUsuario` VARCHAR(150))  begin
	DECLARE idUsuarioN INT;
	SET idUsuarioN = (SELECT idUsuario FROM usuarios WHERE ip = ipUsuario);
	SELECT SUM(total) as subTotal FROM carrito_de_compras WHERE idUsuario = idUsuarioN;
end$$

CREATE DEFINER=`` PROCEDURE `Validar_Usuario` (IN `correoE` VARCHAR(150), IN `contraseñaE` VARCHAR(150))  begin
	SELECT * FROM usuarios WHERE correo = correoE AND contraseña = contraseñaE;
end$$

