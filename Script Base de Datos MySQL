CREATE DEFINER=`` PROCEDURE `Actualizar_Usuario_Informacion` (IN `nombreN` VARCHAR(150), IN `apellidoN` VARCHAR(150), IN `direccionN` VARCHAR(150), IN `apartamentoN` VARCHAR(150), IN `distritoN` VARCHAR(150), IN `regionN` VARCHAR(150), IN `celularN` INT, IN `correoN` VARCHAR(150), IN `dniN` VARCHAR(150), IN `ipU` VARCHAR(150))  begin
